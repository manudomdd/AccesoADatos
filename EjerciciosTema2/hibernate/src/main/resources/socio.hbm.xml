<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
    <class name="com.example.hibernate.entity.Socio" table="pelicula">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="nombre" column="nombre" type="string" length="100"/>
        <property name="correo" column="correo" type="string" length="50"/>
    </class>
</hibernate-mapping>